  "scripts": {
    "start": "yarn with-env node dist/src/index.js",
    "dev": "yarn with-env ts-node-dev --poll src/index.ts",
    "build": "tsc",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e .env --",
    "env:validate": "ts-node ./src/env.ts",
    "prepare": "husky",
    "test": "jest",
    "test:cov": "jest --coverage"
